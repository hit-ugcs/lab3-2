<h1>Grading Worksheet</h1>
<table>
  <tr>
    <th>Student ID</th>
    <th>Student Name (Chinese)</th>
    <th>Nick Name (English)</th>
    <th>Project(40%)</th>
    <th>Final(40%)</th>
    <th>Lab(10%)</th>
    <th>Class(10%)</th>
    <th>Final</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
<% @students.each do |student| %>

  <tr>
    <td><%= student.studentid %></td>
    <td><%= student.name %></td>
    <td><%= student.nickname %></td>
    <td><%= student.project %>%</td>
    <td><%= student.finalgrade %>%</td>
    <td><%= student.labgrade %>%</td>
    <td><%= student.classgrade %>%</td>
    <td><%= student.project+student.finalgrade+student.labgrade+student.classgrade %>%</td>
    <td><%= link_to 'Edit', edit_student_path(student) %>
    <%= link_to 'Show', student %>
    <%= link_to 'Destroy', student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
  <tr>
    <td><br/><%= link_to 'New Student', new_student_path %></td>
    <td></td>
    <td></td>
    <td></td>
    <td></td>
    <td></td>
    <td></td>
    <td></br>
        <% if @pnum<@len %>
            <%= link_to 'Next', list_path(@pid+1) %>
        <% end %></td>
    <td></br>
        <% if @pid>1 %>
            <%= link_to 'Previous', list_path(@pid-1) %>
        <% end %></td>
    <td></td>
  </tr>
  <tr>
    <td></td>
    <td></td>
    <td></td>
    <td></td>
    <td></td>
    <td></td>
    <td><input id="btnClose" type="button" value="Exit" onClick="custom_close()" /></td>
    <td><%= link_to 'Print', students_path %></td>
    <td></br></td>
    <td></td>
  </tr>
</table>

<script language="javascript">
function custom_close(){
if 
(confirm("Are you sure to exitï¼Ÿ")){
window.opener=null;
window.open('','_self');
window.close();
}
else{}
}
</script>






